<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mickstarify.nswdriverknowledgetest">

    <application
        android:name=".NSWDriverKnowledgeTestApplication"
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NSWDriverKnowledgeTest">
        <activity android:name=".QuestionListScreen.QuestionListActivityView"></activity>
        <activity android:name=".QuestionScreen.QuestionActivityView" />
        <activity android:name=".EntryScreen.EntryActivityView">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>